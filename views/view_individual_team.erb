<h1> <%= params[:team_name] %> </h1>

<h2> Players on the team: </h2>

<table class="table">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Position</th>
  </tr>
  <% @teams_players.each do |player| %>
  <tr>
    <td> <%= player[:first_name] %> </td>
    <td> <%= player[:last_name] %> </td>
    <td><a href="/all-players/<%= player[:position] %>"> <%= player[:position] %> </a></td>
  </tr>
  <% end %>
</table>
